{{page-title (t "page-title")}}
<AuModalContainer />
<HeadLayout />
{{#each this.systemAlert.unconfirmedAlerts as |alert|}}
  <Utils::SystemAlert
    @type={{alert.type.label}}
    @title={{alert.title}}
    @message={{alert.message}}
    @onClose={{fn this.systemAlert.confirmAlert alert}}
  />
{{/each}}
{{#if this.currentSession.hasAccessToApplication}}
  <MHeader />
{{/if}}
{{outlet}}

<AuToaster @position="bottom" />
