{{page-title
  (if this.showDraft "Klad ")
  (t "newsletter-overview-pdf-name")
  " "
  (moment-format this.meeting.plannedStart)
  replace=true
}}
<div class="auk-scroll-wrapper__body vlc-hide-scrollbar-on-print">
  <div class="auk-u-m-8 auk-u-m-0@print">
    <div class="auk-container auk-container--center l-printable-newsletter">
      <Utils::LogoHeader />
      <Newsletter::NewsletterPrintHeader
        data-test-newsletter-print---header
        @meeting={{this.meeting}}
      />
      <h1 class="auk-h1 auk-u-text-family-serif">
        {{t "agendaitems-subtitle"}}
      </h1>
      {{#each this.model.notas as |nota|}}
        <div class="auk-u-mb-10">
          <NewsletterItem::Print
            @newsletterItem={{nota.newsletterItem}}
            @agendaitem={{nota.agendaitem}}
            @isDraftView={{this.showDraft}}
            @onSave={{perform this.saveNewsletterItem nota.newsletterItem}}
          />
        </div>
      {{/each}}
      {{#if this.model.announcements.length}}
        <h2 class="auk-h1">
          {{t "announcements-subtitle"}}
        </h2>
        {{#each this.model.announcements as |announcement|}}
          <div class="auk-u-mb-10">
            <NewsletterItem::Print
              @newsletterItem={{announcement.newsletterItem}}
              @agendaitem={{announcement.agendaitem}}
              @isDraftView={{this.showDraft}}
              @onSave={{perform this.saveNewsletterItem announcement.newsletterItem}}
            />
          </div>
        {{/each}}
      {{/if}}
    </div>
  </div>
</div>