<Auk::Navbar>
  <Auk::Toolbar>
    <Auk::Toolbar::Group @position="left">
      <Auk::Tabs>
        <Auk::Tab @route="publications.publication.proofs.documents">
          {{t "documents"}}
        </Auk::Tab>
        <Auk::Tab @route="publications.publication.proofs.requests">
          {{t "requests"}}
        </Auk::Tab>
      </Auk::Tabs>
    </Auk::Toolbar::Group>
    <Auk::Toolbar::Group @position="right">
      <Auk::Toolbar::Item>
        <Auk::Checkbox
          @label={{t "finished"}}
          @checked={{this.model.endDate}}
          {{on "input" this.toggleFinishProof}} />
      </Auk::Toolbar::Item>
    </Auk::Toolbar::Group>
  </Auk::Toolbar>
</Auk::Navbar>

{{outlet}}