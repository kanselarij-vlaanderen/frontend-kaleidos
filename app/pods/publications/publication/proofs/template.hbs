<Auk::Navbar>
  <Auk::Toolbar>
    <Auk::Toolbar::Group @position="left">
      <Auk::Tabs>
        <Auk::Tab
          data-test-route-publications---proofs-tab-documents
          @route="publications.publication.proofs.documents"
        >
          {{t "documents"}}
        </Auk::Tab>
        <Auk::Tab
          data-test-route-publications---proofs-tab-requests
          @route="publications.publication.proofs.requests"
        >
          {{t "requests"}}
        </Auk::Tab>
      </Auk::Tabs>
    </Auk::Toolbar::Group>
    <Auk::Toolbar::Group @position="right">
      <Auk::Toolbar::Item>
        <Auk::Checkbox
          data-test-route-publications---proofs-tab-finished
          @label={{t "finished"}}
          @checked={{this.model.endDate}}
          {{on "input" this.toggleFinishProof}}
        />
      </Auk::Toolbar::Item>
    </Auk::Toolbar::Group>
  </Auk::Toolbar>
</Auk::Navbar>

{{outlet}}