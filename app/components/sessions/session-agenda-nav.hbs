<Auk::Tabs @reversed={{true}}>
  <Auk::Tab @route="agendas.overview" @isHierarchicalBack={{true}}/>
  <Auk::Tab
    @route="agenda.agendaitems.index"
    @query={{if this.currentAgendaItemId (hash anchor=this.currentAgendaItemId)}}
  >
    {{t "overview"}}
  </Auk::Tab>
  {{#if this.loadFirstAgendaitem.isIdle}}
    <Auk::Tab
      @route="agenda.agendaitems.agendaitem"
      @models={{this.modelsForDetailRoute}}
    >
      {{t "detail"}}
    </Auk::Tab>
  {{/if}}
  {{#if currentSession.isEditor}}
    <Auk::Tab @route="agenda.compare">
      {{t "compare"}}
    </Auk::Tab>
  {{/if}}
  <Auk::Tab @route="agenda.documents">
    {{t "documents"}}
  </Auk::Tab>
</Auk::Tabs>