<Auk::Tabs @reversed={{true}}>
  <Auk::Tab
    @route="cases"
    @isHierarchicalBack={{true}}
  >
    {{t "all-cases"}}
  </Auk::Tab>
  <Auk::Tab data-test-subcase-detail-nav-overview @route="cases.case.subcases.subcase.index">
    {{t "overview"}}
  </Auk::Tab>
  <Auk::Tab data-test-subcase-detail-nav-documents @route="cases.case.subcases.subcase.documents">
    {{t "documents"}}
  </Auk::Tab>
  <Auk::Tab data-test-subcase-detail-nav-decisions @route="cases.case.subcases.subcase.decision">
    {{capitalize (t "decision")}}
  </Auk::Tab>
</Auk::Tabs>
